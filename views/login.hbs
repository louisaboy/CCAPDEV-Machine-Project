<header>
    <div class="header-left">
        <a href="/"><img src="images/Pics/TLLogo.png" class="TLLogo" alt="ToonList"/></a>
    </div>
</header>   
<body>
<div class="login-box">
  <div class="lb-header">
    <a href="#" class="active" id="login-box-link">Login</a>
    <a href="/signup" id="signup-box-link">Sign Up</a>
    
  </div>

  <div class="social-login">
    <a href="#">
      <i class="fa fa-facebook fa-lg"></i>
    </a>
    <a href="#">
      <i class="fa fa-google-plus fa-lg"></i>
    </a>
    <a href="#">
      <i class="fa fa-twitter fa-lg"></i>
    </a>
  </div>

  <form class="email-login" id="email-login" method="post">
    <div class="u-form-group">
      <input type="email" name="logemail" placeholder="Email" required=""/>
    </div>

    <div class="u-form-group">
      <input type="password" name="logpass" placeholder="Password" required=""/>
    </div>

    <div class="u-form-group">
      <a href="#" class="forgot-password">Forgot password?</a>
    </div>

    <div class="u-form-group">
      <input class="submit-buttn" type="submit" value="Log In">
    </div>
  </form>
</div>

<script src="js/signup-script.js"></script>